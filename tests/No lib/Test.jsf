/* ===========================================================================

	Test not having a local fwrequire.js file, which doesn't make sense for
	FW extensions but is useful for using tests from RequireJS.

   ======================================================================== */


(function() {

	// we have to save fw.currentScriptDir before calling runScript because that
	// will leave fw.currentScriptDir null
var requirePath = fw.currentScriptDir + "/../../";

if (typeof require != "function" || !require.version) {
	fw.runScript(requirePath + "fwrequire.js"); }

require({ baseUrl: "./", requirePath: requirePath }, [
	"foo"
], function NoLib(
	foo)
{
	var message = "No Lib",
		result = foo.bar(message);

	doh.t(result.indexOf(message) == 0);
	doh.t(result.length > message.length);
});

})();
